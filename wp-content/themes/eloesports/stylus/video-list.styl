.video_list
	width 40%
	//padding-top 10px
	//margin-left 10px
	overflow-x hidden
	overflow-y hidden
	.video_listed
		width 100%
		display flex
		margin 0
		cursor pointer
		padding 15px 10px
		&-thumb
			position relative
			height 69px
			width 140px
			overflow hidden
			margin 0
			img
				position absolute
				width 100%
				top -19px
		&-content
			color white
			margin-left 5px
			margin-right 10px
			&-title
				margin 0
				font-family 'Roboto'
				font-weight 400
				font-size 17px
				color lightgray
			&-info
				color gray
				display flex
				margin-top 8px
				&-date
					margin 0
					margin-right 5px
				&-category
					margin 0
					color lightgray

				
			
	